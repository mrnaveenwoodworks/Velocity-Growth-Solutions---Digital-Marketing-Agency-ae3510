    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.2",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.3",
    "framer-motion": "^10.16.4",
